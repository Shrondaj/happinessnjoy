{
  "name": "shronda-jeanine-company",
  "version": "1.0.0",
  "description": "Shronda Jeanine & Company - Embracing Authenticity, Cultivating Abundance",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && npm run postbuild",
    "postbuild": "cp _headers _redirects dist/ 2>/dev/null || true",
    "preview": "vite preview --host",
    "lint": "eslint . --ext .js,.html --fix --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "optimize": "npm run lint && npm run format && npm run build",
    "clean": "rm -rf dist node_modules/.vite",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "empowerment",
    "coaching",
    "healing",
    "authenticity",
    "abundance",
    "generational-wealth"
  ],
  "author": "Shronda Jeanine & Company",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "vite": "^5.4.8",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "@vitejs/plugin-legacy": "^5.4.2",
    "terser": "^5.34.1",
    "typescript": "^5.6.2",
    "@types/node": "^22.7.4",
    "lightningcss": "^1.27.0",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}